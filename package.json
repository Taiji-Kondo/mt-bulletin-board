{
  "name": "next-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "next dev",
    "dev:path": "pathpida --ignorePath .gitignore --watch --output ./src/__generated__/path",
    "build": "pathpida --ignorePath .gitignore --output ./src/__generated__/path && next build",
    "start": "next start",
    "lint": "run-p lint:*",
    "lint:next": "next lint",
    "lint:eslint": "eslint src --ext .js,.ts,.jsx,.tsx",
    "lint:prettier": "prettier --check ./src",
    "format": "run-s format:*",
    "format:eslint": "eslint src --ext .js,.ts,.jsx,.tsx --fix",
    "format:prettier": "prettier --write ./src",
    "typecheck": "tsc --noEmit",
    "test": "jest --watch",
    "generate": "graphql-codegen --config ./codegen.ts",
    "ncu": "npx npm-check-updates"
  },
  "dependencies": {
    "@emotion/react": "^11.10.0",
    "@mantine/core": "^5.2.4",
    "@mantine/hooks": "^5.2.4",
    "@mantine/next": "^5.2.4",
    "dayjs": "^1.11.5",
    "graphql": "^16.6.0",
    "next": "12.2.5",
    "next-seo": "^5.5.0",
    "next-urql": "^4.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-is": "^18.2.0",
    "recoil": "^0.7.5",
    "tabler-icons-react": "^1.54.0",
    "urql": "^3.0.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.12.0",
    "@graphql-codegen/introspection": "2.2.1",
    "@graphql-codegen/near-operation-file-preset": "^2.4.1",
    "@graphql-codegen/typescript": "2.7.3",
    "@graphql-codegen/typescript-operations": "2.5.3",
    "@graphql-codegen/typescript-urql": "^3.6.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/node": "18.7.13",
    "@types/prettier": "^2.7.0",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@urql/exchange-suspense": "^1.9.2",
    "eslint": "8.20.0",
    "eslint-config-next": "^12.2.3",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sort-destructure-keys": "^1.4.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-strict-dependencies": "^1.0.1",
    "eslint-plugin-typescript-sort-keys": "^2.1.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^29.0.2",
    "jest-environment-jsdom": "^29.0.2",
    "npm-run-all": "^4.1.5",
    "pathpida": "^0.18.1",
    "prettier": "^2.7.1",
    "typescript": "4.7.4"
  },
  "volta": {
    "node": "16.16.0"
  }
}
